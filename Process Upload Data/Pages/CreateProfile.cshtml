@page
@model Process_Upload_Data.Pages.CreateProfileModel
@{
}
<head>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/css/StyleSheet.css">
</head>
<div class="text-center">
    <h1>Create A New Profile</h1>
    <form method="post" cllass="mt-3" enctype="multipart/form-data">
        <div class="col-sm-10">
            <div class="TextBox">
                <input asp-for="USERNAME" type="text" placeholder="Username" class="InputBox" />
            </div>
            <div class="TextBox">
                <input asp-for="PASSWORD" type="text" placeholder="Password" class="InputBox" />
            </div>
        </div>
        <div class="col-sm-10" style="width: 27%;">
            <button type="submit" class="btn btn-primary">Create Profile</button>
        </div>
    </form>
</div>